{
  "mcpServers": {
    "guardrails": {
      "command": "npx",
      "args": ["tsx", "guardrails/index.ts"]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest",
        "--headless"
      ]
    },
    "playwright-test": {
      "command": "npx",
      "args": [
        "playwright",
        "run-test-mcp-server"
      ]
    }
  }
}
